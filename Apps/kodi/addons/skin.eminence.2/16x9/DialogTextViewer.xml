<?xml version="1.0" encoding="UTF-8"?>
<window>
    <!-- textviewer -->
    <defaultcontrol always="true">61</defaultcontrol>
    <controls>
        <include>GlobalOverlay</include>
        <control type="group">
            <top>dialogtop</top>
            <bottom>bigpad</bottom>
            <centerleft>50%</centerleft>
            <width>1200</width>
            <include>Animation_SlideIn</include>
            <include>Animation_FadeOut</include>
            <animation effect="slide" tween="quadratic" easing="out" time="300" start="0,1920" end="0">WindowOpen</animation>
            <animation effect="slide" tween="quadratic" easing="in" time="300" end="0,1920" start="0">WindowClose</animation>
            <include>Dialog_Background</include>
            <control type="group">
                <top>70</top>
                <left>bigpad</left>
                <right>bigpad</right>
                <bottom>bigpad</bottom>
                <include content="Object_Line">
                    <param name="width" value="1160" />
                    <param name="label" value="$INFO[Control.GetLabel(1)]" />
                </include>
                <control type="textbox" id="5">
                    <left>40</left>
                    <right>0</right>
                    <top>bigpad</top>
                    <bottom>pad</bottom>
                    <font>Font-ListInfo-Small</font>
                    <pagecontrol>61</pagecontrol>
                </control>
                <control type="scrollbar" id="61">
                    <left>0</left>
                    <top>bigpad</top>
                    <width>pad</width>
                    <bottom>0</bottom>
                </control>
            </control>
        </control>

        <!-- Heading -->
        <control type="label" id="1"><visible>false</visible></control>

    </controls>

</window>

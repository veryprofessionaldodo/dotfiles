<?xml version="1.0" encoding="UTF-8"?>
<window>
    <defaultcontrol always="true">450</defaultcontrol>
    <controls>
        <include>GlobalOverlay</include>
        <control type="group">
            <include>Animation_SlideIn</include>
            <include>Animation_FadeOut</include>
            <centerleft>50%</centerleft>
            <width>1792</width>
            <bottom>100</bottom>
            <height>676</height>
            <include>Dialog_Background</include>
            <include content="Dialog_Top_IconHeader">
                <param name="label" value="$INFO[Control.GetLabel(411)]" />
                <param name="icon" value="special://skin/extras/icons/filebox.png" />
            </include>
            <control type="group">
                <left>bigpad</left>
                <bottom>bigpad</bottom>
                <width>576</width>
                <height>576</height>
                <include>Object_FlatBackground</include>
                <control type="image">
                    <aspectratio>keep</aspectratio>
                    <texture background="true" fallback="DefaultAddon.png">$INFO[ListItem.Icon]</texture>
                    <visible>String.Contains(ListItem.Icon,Default)</visible>
                </control>
                <control type="image">
                    <aspectratio>keep</aspectratio>
                    <texture background="true" fallback="DefaultAddon.png">$INFO[ListItem.Icon]</texture>
                    <visible>!String.Contains(ListItem.Icon,Default) + !String.Contains(Control.GetLabel(416),*)</visible>
                </control>
                <control type="image">
                    <aspectratio>keep</aspectratio>
                    <texture background="true" fallback="DefaultAddon.png" flipx="true">$INFO[ListItem.Icon]</texture>
                    <visible>!String.Contains(ListItem.Icon,Default) + String.Contains(Control.GetLabel(416),*)</visible>
                </control>
            </control>
            <control type="group">
                <left>616</left>
                <bottom>bigpad</bottom>
                <width>1156</width>
                <height>576</height>
                <include>Object_FlatBackground</include>
            </control>
            <include content="Dialog_ImageList">
                <param name="id" value="450" />
                <param name="moveid" value="9000" />
                <param name="onup" value="450" />
                <param name="ondown" value="450" />
                <param name="left" value="616" />
                <param name="width" value="1156" />
            </include>
            <include content="Dialog_ImageList">
                <param name="id" value="451" />
                <param name="moveid" value="9000" />
                <param name="onup" value="450" />
                <param name="ondown" value="450" />
                <param name="left" value="616" />
                <param name="width" value="1156" />
            </include>
            <!-- Button grouplist -->
            <control type="grouplist" id="9000">
                <right>bigpad</right>
                <top>pad</top>
                <align>right</align>
                <height>60</height>
                <onleft>450</onleft>
                <onright>noop</onright>
                <onup>noop</onup>
                <ondown>450</ondown>
                <orientation>horizontal</orientation>
                <itemgap>pad</itemgap>
                <!-- OK Button -->
                <control type="button" id="413">
                    <width>auto</width>
                    <height>100%</height>
                    <label>186</label>
                    <align>right</align>
                    <texturenofocus />
                    <texturefocus />
                    <focusedcolor>$VAR[HighlightColor]</focusedcolor>
                    <include>Defs_OptionButton</include>
                </control>

                <!-- Cancel Button -->
                <control type="button" id="414">
                    <width>auto</width>
                    <height>100%</height>
                    <label>222</label>
                    <align>right</align>
                    <texturenofocus />
                    <texturefocus />
                    <focusedcolor>$VAR[HighlightColor]</focusedcolor>
                    <include>Defs_OptionButton</include>
                </control>

                <!-- New folder -->
                <control type="button" id="415">
                    <width>auto</width>
                    <height>100%</height>
                    <label>119</label>
                    <visible>Control.IsEnabled(415)</visible>
                    <align>right</align>
                    <texturenofocus />
                    <texturefocus />
                    <focusedcolor>$VAR[HighlightColor]</focusedcolor>
                    <include>Defs_OptionButton</include>
                </control>

                <!-- Mirror image -->
                <control type="radiobutton" id="416">
                    <width>auto</width>
                    <height>100%</height>
                    <label>749</label>
                    <visible>Control.IsEnabled(416)</visible>
                    <align>right</align>
                    <texturenofocus />
                    <texturefocus />
                    <focusedcolor>$VAR[HighlightColor]</focusedcolor>
                    <include>Defs_OptionButton</include>
                </control>

            </control>
        </control>

        <!-- Hidden Label -->
        <control type="label" id="411"><visible>false</visible><font /></control>
    </controls>

</window>
<?xml version="1.0" encoding="UTF-8"?>
<includes>

    <default type="label">
        <textcolor>ListLabel</textcolor>
        <selectedcolor>$VAR[HighlightColor2]</selectedcolor>
        <disabledcolor>Disabled</disabledcolor>
        <font>Font-Button</font>
        <aligny>center</aligny>
        <align>left</align>
    </default>

    <default type="fadelabel">
        <font>Font-Button</font>
        <textcolor>ListLabel</textcolor>
        <selectedcolor>$VAR[HighlightColor2]</selectedcolor>
        <disabledcolor>Disabled</disabledcolor>
    </default>

    <default type="button">
        <pulseonselect>false</pulseonselect>
        <font>Font-Button</font>
        <textcolor>ListLabel</textcolor>
        <selectedcolor>$VAR[HighlightColor2]</selectedcolor>
        <disabledcolor>Disabled</disabledcolor>
    </default>

    <default type="togglebutton">
        <font>Font-Button</font>
        <pulseonselect>false</pulseonselect>
        <textcolor>ListLabel</textcolor>
        <selectedcolor>$VAR[HighlightColor2]</selectedcolor>
        <disabledcolor>Disabled</disabledcolor>
    </default>

    <default type="radiobutton">
        <font>Font-Button</font>
        <radioposx>1350</radioposx>
        <radiowidth>32</radiowidth>
        <radioheight>32</radioheight>
        <textoffsetx>30</textoffsetx>
        <textcolor>ListLabel</textcolor>
        <selectedcolor>$VAR[HighlightColor2]</selectedcolor>
        <disabledcolor>Disabled</disabledcolor>
        <textureradioonfocus colordiffuse="Selected">common/radio-on.png</textureradioonfocus>
        <textureradioonnofocus colordiffuse="ListLabel">common/radio-on.png</textureradioonnofocus>
        <textureradioofffocus colordiffuse="Disabled">common/radio-off.png</textureradioofffocus>
        <textureradiooffnofocus colordiffuse="Disabled">common/radio-off.png</textureradiooffnofocus>
        <textureradiooffdisabled colordiffuse="Disabled">common/radio-off.png</textureradiooffdisabled>
        <textureradioondisabled colordiffuse="Disabled">common/radio-on.png</textureradioondisabled>
        <pulseonselect>false</pulseonselect>
    </default>

    <default type="spincontrolex">
        <font>Font-Button</font>
        <pulseonselect>false</pulseonselect>
        <spinwidth>32</spinwidth>
        <spinheight>32</spinheight>
        <textureup flipy="true" colordiffuse="Disabled">common/spin-down.png</textureup>
        <texturedown colordiffuse="Disabled">common/spin-down.png</texturedown>
        <textureupfocus flipy="true" colordiffuse="Selected">common/spin-down.png</textureupfocus>
        <texturedownfocus colordiffuse="Selected">common/spin-down.png</texturedownfocus>
        <textureupdisabled flipy="true" colordiffuse="Disabled">common/spin-down.png</textureupdisabled>
        <texturedowndisabled colordiffuse="Disabled">common/spin-down.png</texturedowndisabled>
        <textcolor>ListLabel</textcolor>
        <selectedcolor>$VAR[HighlightColor2]</selectedcolor>
        <disabledcolor>Disabled</disabledcolor>
    </default>

    <default type="spincontrol">
        <font>Font-Button</font>
        <pulseonselect>false</pulseonselect>
        <spinwidth>32</spinwidth>
        <spinheight>32</spinheight>
        <textureup flipy="true" colordiffuse="Disabled">common/spin-down.png</textureup>
        <texturedown colordiffuse="Disabled">common/spin-down.png</texturedown>
        <textureupfocus flipy="true" colordiffuse="Selected">common/spin-down.png</textureupfocus>
        <texturedownfocus colordiffuse="Selected">common/spin-down.png</texturedownfocus>
        <textureupdisabled flipy="true" colordiffuse="Disabled">common/spin-down.png</textureupdisabled>
        <texturedowndisabled colordiffuse="Disabled">common/spin-down.png</texturedowndisabled>
        <textcolor>ListLabel</textcolor>
        <selectedcolor>$VAR[HighlightColor2]</selectedcolor>
        <disabledcolor>Disabled</disabledcolor>
    </default>

    <default type="sliderex">
        <font>Font-Button</font>
        <pulseonselect>false</pulseonselect>
        <sliderwidth>230</sliderwidth>
        <sliderheight>23</sliderheight>
        <orientation>horizontal</orientation>
        <texturesliderbar>common/slider_bg.png</texturesliderbar>
        <textureslidernib border="2">common/SliderNibNF.png</textureslidernib>
        <textureslidernibfocus border="2">common/SliderNibFO.png</textureslidernibfocus>
        <textcolor>ListLabel</textcolor>
        <selectedcolor>$VAR[HighlightColor2]</selectedcolor>
        <disabledcolor>Disabled</disabledcolor>
    </default>

    <default type="slider">
        <pulseonselect>false</pulseonselect>
        <texturesliderbar>common/slider_bg.png</texturesliderbar>
        <textureslidernib border="2">common/SliderNibNF.png</textureslidernib>
        <textureslidernibfocus border="2">common/SliderNibFO.png</textureslidernibfocus>
        <orientation>horizontal</orientation>
        <textcolor>ListLabel</textcolor>
        <selectedcolor>$VAR[HighlightColor2]</selectedcolor>
        <disabledcolor>Disabled</disabledcolor>
    </default>

    <default type="progress">
        <texturebg colordiffuse="PosterBorder">osd/progress-bg.png</texturebg>
        <lefttexture colordiffuse="$VAR[HighlightColor]">osd/progress-end.png</lefttexture>
        <midtexture colordiffuse="$VAR[HighlightColor]">osd/progress-end.png</midtexture>
        <righttexture colordiffuse="$VAR[HighlightColor]">osd/progress-end.png</righttexture>
        <overlaytexture />
        <textcolor>ListLabel</textcolor>
        <selectedcolor>$VAR[HighlightColor2]</selectedcolor>
        <disabledcolor>Disabled</disabledcolor>
    </default>

    <default type="textbox">
        <align>justify</align>
        <font>Font-ListInfo</font>
        <textcolor>ListLabel</textcolor>
        <selectedcolor>Selected</selectedcolor>
        <autoscroll delay="8000" time="4000" repeat="12000">True</autoscroll>
    </default>

    <default type="edit">
        <font>Font-Button</font>
        <textcolor>ListLabel</textcolor>
        <selectedcolor>$VAR[HighlightColor2]</selectedcolor>
        <disabledcolor>Disabled</disabledcolor>
    </default>

    <default type="scrollbar">
        <texturesliderbackground colordiffuse="PosterBorder">common/white.png</texturesliderbackground>
        <texturesliderbar colordiffuse="Disabled">common/white.png</texturesliderbar>
        <texturesliderbarfocus colordiffuse="$VAR[HighlightColor]">common/white.png</texturesliderbarfocus>
        <textureslidernib />
        <textureslidernibfocus />
        <showonepage>true</showonepage>
    </default>

</includes>